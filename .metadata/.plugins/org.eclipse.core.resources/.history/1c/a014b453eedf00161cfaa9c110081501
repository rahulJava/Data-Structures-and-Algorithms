
public class Human {
	String name;
	int age;
	String heightInInches;
	String eyeColor;
	public Human()
	{
		
	}
	public void speak()
	{
		System.out.println("Hello, my name is:"+name);
		System.out.println("I am:"+age+"years old");
		System.out.println("My eye color is "+eyeColor);
		System.out.println("I am:"+heightInInches+"tall");
		
	}

}
