private String getMetaFieldsFromDB(String []loginMetafields){
	boolean    isMetafieldFoundInRequest=false;
	String     metaFieldFromDB="";            
	for(int i=0;i<loginMetafields.length;i++)
	{
		metaFieldFromDB=loginMetafields[i];
		if(getUserCredential(metaFieldFromDB)!=null)
		{
			isMetafieldFoundInRequest=true;
			break;
		}
	}
	if(!isMetafieldFoundInRequest) {
		throw new Exception("Different LOGIN metafields in DB!!!");
	}

	return metaFieldFromDB;
}
}
